{
	"cleanUrls": true,
	"trailingSlash": false,
	"github": {
		"silent": true
	},
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "Permissions-Policy",
					"value": "interest-cohort=()"
				}
			]
		},
		{
			"source": "(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-cache"
				}
			]
		},
		{
			"source": "(.*).(css|js)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}